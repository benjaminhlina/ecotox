         title        = "The relationship between thermal physiology and lampricide
         sensitivity in larval sea lamprey (Petromyzon marinus)",
         author       = personList(as.person("Benjamin L. Hlina"),
                                   as.person("Oana Birceanu"),
                                   as.person("Christopher S. Robinson"),
                                   as.person("Hadi Dhiyebi"),
                                   as.person("Michael P. Wilkie")),
         journal      = "Journal of Great Lakes Research",
         year         = "In Press",

         textVersion  =
           paste("Hlina, B.L., Birceanu, O., Robinson, C.S., Dhiyebi, H., Wilkie, M.P.",
                 "In Press.",
                 "The relationship between thermal physiology and lampricide sensitivity in
                 larval sea lamprey (Petromyzon marinus).",
                 "Journal of Great Lakes Research"))
